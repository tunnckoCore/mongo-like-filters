{
  "name": "mongo-like-filters",
  "version": "0.1.3",
  "license": "MPL-2.0",
  "description": "A lightweight TypeScript library with full type-safety for filtering objects using MongoDB-style query syntax",
  "homepage": "https://github.com/tunnckoCore/mongo-like-filters",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tunnckoCore/mongo-like-filters.git"
  },
  "type": "module",
  "module": "src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --dts --format esm --target esnext",
    "check": "tsc --noEmit --allowImportingTsExtensions test/examples.ts",
    "prepublishOnly": "bun run test && bun run build",
    "test": "bun test"
  },
  "resolutions": {
    "@types/glob": "8.1.0",
    "@types/minimatch": "5.1.2",
    "minimatch": "5.1.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@types/minimatch": "5.1.2",
    "bun-types": "latest",
    "eslint": "^9.27.0",
    "eslint-config-xaxa": "^28",
    "prettier": "^3.5.3",
    "prettier-plugin-pkgjson": "^0.3.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3"
  }
}
